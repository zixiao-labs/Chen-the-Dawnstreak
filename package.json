{
  "name": "chen-the-dawnstreak",
  "version": "3.0.0",
  "description": "赤刃明霄陈-全栈React框架但是不依赖Vercel(应该是全NPM明日方舟浓度最高的包了吧)",
  "keywords": [
    "react",
    "framework",
    "arknights"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zixiao-labs/Chen-the-Dawnstreak"
  },
  "author": "amiya167",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "chen-the-dawnstreak": "./dist/cli/index.js",
    "chen": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./vite-plugin": {
      "types": "./dist/vite-plugin/index.d.ts",
      "import": "./dist/vite-plugin/index.js"
    },
    "./vite-plugin/client": {
      "types": "./dist/vite-plugin/client.d.ts"
    },
    "./ssr": {
      "types": "./dist/ssr/index.d.ts",
      "import": "./dist/ssr/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && chmod +x dist/cli/index.js && cp src/vite-plugin/client.d.ts dist/vite-plugin/client.d.ts",
    "prepublishOnly": "tsc && chmod +x dist/cli/index.js && cp src/vite-plugin/client.d.ts dist/vite-plugin/client.d.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@mdui/icons": "^1.0.3",
    "mdui": "^2.1.4",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router": "^7.13.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "jsdom": "^28.1.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "vite": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  }
}
